<tool id="retip-train" name="Retip training" version="{{tool_version}}">
	<description>the Keras model to predit retention times</description>
    <requirements>
      <container type="docker">{{container_registry}}:{{tool_version}}</container>
    </requirements>
	<command detect_errors="exit_code"><![CDATA[
            /run.sh trainKeras.R '$descr_train' '$output_file2'
      ]]>
    </command>
	<inputs>
                <param name="descr_train" label="Select Descriptors.Feather Dataset" type="data" format="feather" optional="false"/>
	</inputs>
    <outputs>
        <data format="h5" name="output_file2" label="Model.hdf5 Dataset" from_work_dir=""/>
    </outputs>
    <help>Train the Keras model </help>
</tool>
